<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <LangVersion>7.1</LangVersion>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <!-- In order to make the build process copy all referenced dll files from NuGet
         packages from the cache folder into the build output, set this property inside a <PropertyGroup>: -->
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <DSharpPlusVersion>4.0.0-beta-00368</DSharpPlusVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DSharpPlus" Version="$(DSharpPlusVersion)" />
    <PackageReference Include="DSharpPlus.CommandsNext" Version="$(DSharpPlusVersion)" />
    <PackageReference Include="DSharpPlus.Interactivity" Version="$(DSharpPlusVersion)" />
    <PackageReference Include="DSharpPlus.WebSocket.WebSocket4NetCore" Version="$(DSharpPlusVersion)" />
    <PackageReference Include="Humanizer.Core" Version="2.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="2.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.InMemory" Version="2.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="2.0.0" />
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="2.0.0" />
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL.Design" Version="1.1.1" />
    <PackageReference Include="RestSharp" Version="105.2.3" />
    <PackageReference Include="RestSharp.Newtonsoft.Json" Version="1.3.0" />
  </ItemGroup>
  <ItemGroup>
    <DotNetCliToolReference Include="Microsoft.EntityFrameworkCore.Tools.DotNet" Version="2.0.0" />
  </ItemGroup>
</Project>
